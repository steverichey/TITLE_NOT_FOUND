<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="LowRez" file="LowRez" main="Main" version="0.0.1" company="HaxeFlixel" />
	<app preloader="flixel.system.FlxPreloader" />
	<set name="SWF_VERSION" value="11.8" />
	<window width="256" height="256" fps="60" background="#000000" hardware="true" vsync="true" />
	<window if="web" orientation="portrait"  />
	<window if="desktop" orientation="landscape" fullscreen="false" resizable="true" />
	<window if="mobile" orientation="landscape" fullscreen="true" width="0" height="0" />
	<set name="BUILD_DIR" value="export" />
	<classpath name="source" />
	<assets path="sound">
		<sound path="hurt.mp3" id="Hurt"/>
		<sound path="move.mp3" id="Move"/>
		<sound path="slide.mp3" id="Slide"/>
		<sound path="goody.mp3" id="Goody"/>
	</assets>
	<haxelib name="flixel"/>
	<haxedef name="FLX_NO_DEBUG" unless="debug" />
	<haxedef name="NAPE_RELEASE_BUILD" unless="debug" />
</project>